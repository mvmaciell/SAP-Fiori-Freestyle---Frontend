<mvc:View controllerName="zmvov.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:t="sap.ui.table"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"> 

    <Page id="page" title="{i18n>title}">
          <Text id="_IDGenText" core:require="{ formatMessage: 'sap/base/strings/formatMessage' }"
          text="{
            parts:[
              {path:'i18n>welcomeMsg'},
              {path:'/usuario/nome'}
            ],
            formatter: 'formatMessage'
        }"/>

        <Button id="_IDGenButton" text="Exibir Mensagem" press="onExibirMensagem"/>

    </Page>

</mvc:View> 